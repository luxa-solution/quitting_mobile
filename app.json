{
  "expo": {
    "name": "Template App",
    "slug": "template",
    "version": "0.0.1",
    "newArchEnabled": true,
    "scheme": "com.luxasolution.template",
    "platforms": ["ios", "android"],
    "plugins": ["expo-router", "expo-localization", "expo-font"],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.luxasolution.template"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.luxasolution.template",
      "edgeToEdgeEnabled": true
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
