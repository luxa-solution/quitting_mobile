{
  "expo": {
    "name": "Quitting",
    "slug": "quittingmobile",
    "version": "0.0.1",
    "newArchEnabled": true,
    "scheme": "com.luxasolution.template",
    "platforms": ["ios", "android"],
    "plugins": ["expo-router", "expo-localization", "expo-font"],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.luxasolution.quitting"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/app-icons/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.luxasolution.quitting",
      "edgeToEdgeEnabled": true
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "da8523e0-5bbe-46ba-8a73-73b919fce765"
      }
    },
    "owner": "luxa-digitals"
  }
}
